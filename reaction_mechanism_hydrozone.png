<svg viewBox="0 0 1200 800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <style>
      .atom-label { font-family: Arial, sans-serif; font-size: 12px; font-weight: bold; }
      .bond { stroke: #000; stroke-width: 2; fill: none; }
      .forming-bond { stroke: #ff6b6b; stroke-width: 2; stroke-dasharray: 5,5; fill: none; }
      .breaking-bond { stroke: #4ecdc4; stroke-width: 2; stroke-dasharray: 3,3; fill: none; }
      .hydrogen-bond { stroke: #87ceeb; stroke-width: 1; stroke-dasharray: 2,2; fill: none; }
      .title { font-family: Arial, sans-serif; font-size: 14px; font-weight: bold; text-anchor: middle; }
      .state-box { fill: none; stroke: #333; stroke-width: 1; }
      .energy-arrow { stroke: #666; stroke-width: 2; marker-end: url(#arrowhead); }
      .mechanism-arrow { stroke: #ff4444; stroke-width: 3; marker-end: url(#redarrowhead); }
    </style>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#666"/>
    </marker>
    <marker id="redarrowhead" markerWidth="12" markerHeight="8" refX="11" refY="4" orient="auto">
      <polygon points="0 0, 12 4, 0 8" fill="#ff4444"/>
    </marker>
  </defs>

  <!-- Title -->
  <text x="600" y="25" class="title" style="font-size: 16px;">QM/MM EVB Mechanism: Hydrazone Formation via Hemiaminal Intermediate</text>
  <text x="600" y="45" class="title" style="font-size: 12px; fill: #666;">LmrR_pAF Designer Enzyme with pAF15, H93, and R92 Catalytic Residues</text>

  <!-- RS: Reactant State -->
  <g id="reactant-state">
    <rect x="20" y="80" width="320" height="200" class="state-box" rx="5"/>
    <text x="180" y="100" class="title">RS: Reactant State</text>
    
    <!-- para-hydroxy benzaldehyde (substrate) -->
    <g id="benzaldehyde">
      <!-- Benzene ring -->
      <polygon points="80,140 100,130 120,130 140,140 140,160 120,170 100,170 80,160" fill="none" stroke="#000" stroke-width="2"/>
      <!-- OH group -->
      <line x1="140" y1="140" x2="155" y2="130" class="bond"/>
      <text x="158" y="128" class="atom-label" fill="#ff0000">OH</text>
      <!-- CHO group -->
      <line x1="80" y1="140" x2="65" y2="130" class="bond"/>
      <text x="45" y="128" class="atom-label">CHO</text>
      <text x="58" y="138" class="atom-label" style="font-size: 10px;">C1</text>
      <text x="43" y="118" class="atom-label" style="font-size: 10px;">O1</text>
    </g>
    
    <!-- pAF15 residue -->
    <g id="paf15-rs">
      <text x="240" y="140" class="atom-label" fill="#0066cc">pAF15</text>
      <text x="250" y="155" class="atom-label" style="font-size: 10px;">N15</text>
      <line x1="245" y1="145" x2="245" y2="165" class="bond"/>
      <text x="235" y="175" class="atom-label">NH₂</text>
    </g>
    
    <!-- H93 (protonated) -->
    <g id="his93-rs">
      <text x="180" y="200" class="atom-label" fill="#8B4513">H93</text>
      <polygon points="180,210 190,210 195,220 190,230 180,230 175,220" fill="none" stroke="#8B4513" stroke-width="1"/>
      <text x="175" y="245" class="atom-label" style="font-size: 10px;">Nδ-H</text>
    </g>
    
    <!-- R92 -->
    <g id="arg92-rs">
      <text x="280" y="200" class="atom-label" fill="#006400">R92</text>
      <text x="285" y="215" class="atom-label" style="font-size: 10px;">NH₂⁺</text>
    </g>
    
    <!-- Distance labels -->
    <text x="150" y="190" style="font-size: 10px; fill: #666;">d(C1-N15) = 3.2 Å</text>
  </g>

  <!-- Arrow RS -> TS1 -->
  <line x1="350" y1="180" x2="390" y2="180" class="mechanism-arrow"/>
  <text x="370" y="175" style="font-size: 10px; text-anchor: middle;">nucleophilic attack</text>

  <!-- TS1: Hemiaminal Formation Transition State -->
  <g id="ts1-state">
    <rect x="410" y="80" width="320" height="200" class="state-box" rx="5"/>
    <text x="570" y="100" class="title">TS1: Hemiaminal Formation</text>
    
    <!-- Forming hemiaminal -->
    <g id="hemiaminal-ts1">
      <!-- Benzene ring -->
      <polygon points="480,140 500,130 520,130 540,140 540,160 520,170 500,170 480,160" fill="none" stroke="#000" stroke-width="2"/>
      <!-- OH group -->
      <line x1="540" y1="140" x2="555" y2="130" class="bond"/>
      <text x="558" y="128" class="atom-label" fill="#ff0000">OH</text>
      
      <!-- Forming C-N bond -->
      <line x1="480" y1="140" x2="465" y2="130" class="forming-bond"/>
      <text x="445" y="128" class="atom-label">C1</text>
      <line x1="465" y1="130" x2="465" y2="110" class="forming-bond"/>
      <text x="455" y="108" class="atom-label" fill="#0066cc">N15</text>
      
      <!-- Partial OH formation -->
      <line x1="445" y1="128" x2="430" y2="138" class="forming-bond"/>
      <text x="415" y="145" class="atom-label" fill="#ff0000">OH⁻</text>
    </g>
    
    <!-- H93 acting as proton shuttle -->
    <g id="his93-ts1">
      <text x="580" y="200" class="atom-label" fill="#8B4513">H93</text>
      <polygon points="580,210 590,210 595,220 590,230 580,230 575,220" fill="none" stroke="#8B4513" stroke-width="1"/>
      <!-- H-bond to forming OH -->
      <line x1="575" y1="220" x2="445" y2="145" class="hydrogen-bond"/>
      <text x="510" y="180" style="font-size: 9px; fill: #87ceeb;">H-bond</text>
    </g>
    
    <!-- R92 stabilizing -->
    <g id="arg92-ts1">
      <text x="650" y="200" class="atom-label" fill="#006400">R92</text>
      <text x="655" y="215" class="atom-label" style="font-size: 10px;">NH₂⁺</text>
      <!-- H-bond to OH -->
      <line x1="650" y1="205" x2="430" y2="138" class="hydrogen-bond"/>
    </g>
    
    <!-- Bond parameters -->
    <text x="520" y="190" style="font-size: 10px; fill: #666;">d(C1-N15) = 1.8 Å</text>
    <text x="520" y="205" style="font-size: 10px; fill: #666;">d(C1-OH) = 1.6 Å</text>
  </g>

  <!-- Arrow TS1 -> INT -->
  <line x1="740" y1="180" x2="780" y2="180" class="mechanism-arrow"/>

  <!-- INT: Hemiaminal Intermediate -->
  <g id="intermediate-state">
    <rect x="800" y="80" width="320" height="200" class="state-box" rx="5"/>
    <text x="960" y="100" class="title">INT: Hemiaminal Intermediate</text>
    
    <!-- Hemiaminal structure -->
    <g id="hemiaminal-int">
      <!-- Benzene ring -->
      <polygon points="870,140 890,130 910,130 930,140 930,160 910,170 890,170 870,160" fill="none" stroke="#000" stroke-width="2"/>
      <!-- OH group on ring -->
      <line x1="930" y1="140" x2="945" y2="130" class="bond"/>
      <text x="948" y="128" class="atom-label" fill="#ff0000">OH</text>
      
      <!-- Formed C-N bond -->
      <line x1="870" y1="140" x2="855" y2="130" class="bond"/>
      <text x="835" y="128" class="atom-label">C1</text>
      <line x1="855" y1="130" x2="855" y2="110" class="bond"/>
      <text x="845" y="108" class="atom-label" fill="#0066cc">N15</text>
      
      <!-- OH on hemiaminal carbon -->
      <line x1="835" y1="128" x2="820" y2="138" class="bond"/>
      <text x="805" y="145" class="atom-label" fill="#ff0000">OH</text>
      <text x="825" y="115" class="atom-label" style="font-size: 10px;">C1</text>
    </g>
    
    <!-- H93 H-bonded -->
    <g id="his93-int">
      <text x="970" y="200" class="atom-label" fill="#8B4513">H93</text>
      <polygon points="970,210 980,210 985,220 980,230 970,230 965,220" fill="none" stroke="#8B4513" stroke-width="1"/>
      <line x1="965" y1="220" x2="820" y2="138" class="hydrogen-bond"/>
      <text x="890" y="175" style="font-size: 9px; fill: #87ceeb;">δ-N···H-O</text>
    </g>
    
    <!-- R92 -->
    <g id="arg92-int">
      <text x="1040" y="200" class="atom-label" fill="#006400">R92</text>
      <text x="1045" y="215" class="atom-label" style="font-size: 10px;">NH₂⁺</text>
      <line x1="1040" y1="205" x2="820" y2="138" class="hydrogen-bond"/>
    </g>
    
    <text x="910" y="190" style="font-size: 10px; fill: #666;">d(C1-N15) = 1.47 Å</text>
    <text x="910" y="205" style="font-size: 10px; fill: #666;">d(C1-OH) = 1.43 Å</text>
  </g>

  <!-- Arrow to TS2 -->
  <line x1="580" y1="320" x2="580" y2="360" class="mechanism-arrow"/>
  <text x="600" y="340" style="font-size: 10px;">dehydration</text>

  <!-- TS2: Dehydration Transition State -->
  <g id="ts2-state">
    <rect x="410" y="380" width="320" height="200" class="state-box" rx="5"/>
    <text x="570" y="400" class="title">TS2: Dehydration Transition State</text>
    
    <!-- Dehydrating hemiaminal -->
    <g id="hemiaminal-ts2">
      <!-- Benzene ring -->
      <polygon points="480,440 500,430 520,430 540,440 540,460 520,470 500,470 480,460" fill="none" stroke="#000" stroke-width="2"/>
      <!-- OH group -->
      <line x1="540" y1="440" x2="555" y2="430" class="bond"/>
      <text x="558" y="428" class="atom-label" fill="#ff0000">OH</text>
      
      <!-- C-N bond (stable) -->
      <line x1="480" y1="440" x2="465" y2="430" class="bond"/>
      <text x="445" y="428" class="atom-label">C1</text>
      <line x1="465" y1="430" x2="465" y2="410" class="forming-bond"/>
      <text x="455" y="408" class="atom-label" fill="#0066cc">N15⁺</text>
      
      <!-- Breaking O-H bond -->
      <line x1="445" y1="428" x2="430" y2="438" class="breaking-bond"/>
      <text x="410" y="445" class="atom-label" fill="#ff0000">OH₂⁺</text>
    </g>
    
    <!-- H93 facilitating proton transfer -->
    <g id="his93-ts2">
      <text x="580" y="500" class="atom-label" fill="#8B4513">H93</text>
      <polygon points="580,510 590,510 595,520 590,530 580,530 575,520" fill="none" stroke="#8B4513" stroke-width="1"/>
      <!-- Proton transfer -->
      <line x1="575" y1="520" x2="430" y2="438" class="hydrogen-bond"/>
      <text x="500" y="475" style="font-size: 9px; fill: #87ceeb;">proton transfer</text>
    </g>
    
    <!-- R92 -->
    <g id="arg92-ts2">
      <text x="650" y="500" class="atom-label" fill="#006400">R92</text>
      <text x="655" y="515" class="atom-label" style="font-size: 10px;">NH₂⁺</text>
      <line x1="650" y1="505" x2="430" y2="438" class="hydrogen-bond"/>
    </g>
    
    <text x="520" y="490" style="font-size: 10px; fill: #666;">d(C1-OH) = 2.1 Å</text>
    <text x="520" y="505" style="font-size: 10px; fill: #666;">∠(C1=N15) forming</text>
  </g>

  <!-- Arrow TS2 -> PS -->
  <line x1="740" y1="480" x2="780" y2="480" class="mechanism-arrow"/>

  <!-- PS: Product State (Iminium Ion) -->
  <g id="product-state">
    <rect x="800" y="380" width="320" height="200" class="state-box" rx="5"/>
    <text x="960" y="400" class="title">PS: Iminium Ion Product</text>
    
    <!-- Iminium ion -->
    <g id="iminium-ps">
      <!-- Benzene ring -->
      <polygon points="870,440 890,430 910,430 930,440 930,460 910,470 890,470 870,460" fill="none" stroke="#000" stroke-width="2"/>
      <!-- OH group -->
      <line x1="930" y1="440" x2="945" y2="430" class="bond"/>
      <text x="948" y="428" class="atom-label" fill="#ff0000">OH</text>
      
      <!-- C=N double bond -->
      <line x1="870" y1="440" x2="855" y2="430" class="bond"/>
      <line x1="870" y1="444" x2="855" y2="434" class="bond"/>
      <text x="835" y="428" class="atom-label">C1</text>
      <line x1="855" y1="430" x2="855" y2="410" class="bond"/>
      <text x="845" y="408" class="atom-label" fill="#0066cc">N15⁺</text>
    </g>
    
    <!-- Water molecule -->
    <g id="water">
      <text x="820" y="480" class="atom-label" fill="#4169E1">H₂O</text>
    </g>
    
    <!-- H93 (regenerated) -->
    <g id="his93-ps">
      <text x="970" y="500" class="atom-label" fill="#8B4513">H93</text>
      <polygon points="970,510 980,510 985,520 980,530 970,530 965,520" fill="none" stroke="#8B4513" stroke-width="1"/>
    </g>
    
    <!-- R92 -->
    <g id="arg92-ps">
      <text x="1040" y="500" class="atom-label" fill="#006400">R92</text>
      <text x="1045" y="515" class="atom-label" style="font-size: 10px;">NH₂⁺</text>
    </g>
    
    <text x="910" y="490" style="font-size: 10px; fill: #666;">d(C1=N15) = 1.28 Å</text>
  </g>

  <!-- Energy Profile Arrow -->
  <line x1="50" y1="650" x2="1100" y2="650" class="energy-arrow"/>
  <text x="575" y="670" class="title">Reaction Coordinate</text>
  
  <!-- Energy levels -->
  <text x="180" y="640" style="font-size: 10px;">E = 0.0</text>
  <text x="570" y="620" style="font-size: 10px;">E = +15.2 kcal/mol</text>
  <text x="960" y="635" style="font-size: 10px;">E = +6.5 kcal/mol</text>
  <text x="570" y="355" style="font-size: 10px;">E = +8.7 kcal/mol</text>
  <text x="960" y="355" style="font-size: 10px;">E = -2.1 kcal/mol</text>

  <!-- Key bond parameters and EVB setup notes -->
  <g id="evb-notes">
    <rect x="50" y="700" width="1100" height="80" fill="#f0f8ff" stroke="#4682b4" stroke-width="1" rx="5"/>
    <text x="600" y="720" class="title" style="font-size: 14px;">EVB Setup Parameters</text>
    <text x="70" y="740" style="font-size: 11px; font-weight: bold;">Key Bonds to Monitor:</text>
    <text x="70" y="755" style="font-size: 10px;">• Forming C1-N15 bond: RS (3.2 Å) → TS1 (1.8 Å) → INT (1.47 Å) → PS (1.28 Å, double bond)</text>
    <text x="70" y="770" style="font-size: 10px;">• Breaking C1-OH bond: INT (1.43 Å) → TS2 (2.1 Å) → PS (eliminated as H₂O)</text>
    
    <text x="600" y="740" style="font-size: 11px; font-weight: bold;">QM Region Atoms:</text>
    <text x="600" y="755" style="font-size: 10px;">• pAF15 side chain (N15, aromatic ring)</text>
    <text x="600" y="770" style="font-size: 10px;">• Substrate (benzaldehyde, C1, O1, OH groups)</text>
    
    <text x="900" y="740" style="font-size: 11px; font-weight: bold;">MM Region:</text>
    <text x="900" y="755" style="font-size: 10px;">• H93 imidazole ring (proton shuttle)</text>
    <text x="900" y="770" style="font-size: 10px;">• R92 guanidinium group (stabilization)</text>
  </g>
</svg>